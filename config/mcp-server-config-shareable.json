{
  "mcpServers": {
    "memory-context-manager-v2": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "memory-context-manager-v2:latest",
        "python",
        "main.py"
      ],
      "env": {
        "MCP_LOG_LEVEL": "INFO",
        "OLLAMA_BASE_URL": "http://host.docker.internal:11434"
      }
    }
  }
}
